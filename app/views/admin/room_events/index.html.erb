<p id="notice"><%= notice %></p>

<h1>Listing Room Events</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @room_events.each do |room_event| %>
      <tr>
        <td><%= link_to 'Show', room_event %></td>
        <td><%= link_to 'Edit', edit_room_event_path(room_event) %></td>
        <td><%= link_to 'Destroy', room_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Room event', new_room_event_path %>
